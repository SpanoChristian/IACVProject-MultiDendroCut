function [gScore, dataFidelity, modelComplexity] = getGricScore(rSqr,sigma,r,d,k,lambda1, lambda2)
%GETGRICSCORE 
% k number of parameters;
% d dimension of the manifold
% r dimension of the space

disp("GET GRIC SCORE FUNCTION CALLED " + rSqr + " SIGMA : " + sigma)
disp("RESULT : " + rSqr./sigma^2)

n = numel(rSqr);

disp(["Num Points : " + n]; ...
     ["Min Res    : " + ])

dataFidelity = sum(min(1/100*rSqr./sigma^2, 20*(r-d)));
modelComplexity = n^(3/2) / (1 + log(n));
gScore = dataFidelity + modelComplexity;

end

